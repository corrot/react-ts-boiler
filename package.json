{
	"name": "react-template",
	"version": "1.0.0",
	"description": "",
	"main": "./src/index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=development webpack-dev-server --open --config './config/webpack.dev.ts'",
		"build": "cross-env NODE_ENV=production && rimraf ./dist && webpack --config './config/webpack.prod.ts'",
		"clean": "rimraf ./dist",
		"lint": "npm run lint:ts && npm run lint:scss",
		"lint:ts": "tslint -p tsconfig.json -c tslint.json 'src/**/*.{ts,tsx}'",
		"lint:scss": "stylelint './src/**/*.scss' --config .stylelintrc",
		"format": "prettier --write src/**/*.{ts,tsx}",
		"test": "jest",
		"test:update": "npm test -- -u"
	},
	"dependencies": {
		"connected-react-router": "6.5.2",
		"history": "4.10.1",
		"normalize.css": "8.0.1",
		"react": "16.10.1",
		"react-dom": "16.10.1",
		"react-inlinesvg": "1.1.7",
		"react-loadable": "5.5.0",
		"react-redux": "7.1.1",
		"react-router": "5.1.2",
		"react-router-dom": "5.1.2",
		"redux": "4.0.4",
		"redux-devtools-extension": "2.13.8",
		"redux-thunk": "^2.3.0",
		"styled-components": "^4.4.0"
	},
	"devDependencies": {
		"@hot-loader/react-dom": "16.9.0",
		"@types/copy-webpack-plugin": "5.0.0",
		"@types/dotenv-webpack": "^1.7.0",
		"@types/enzyme": "3.10.3",
		"@types/html-webpack-plugin": "3.2.1",
		"@types/jest": "24.0.18",
		"@types/mini-css-extract-plugin": "0.8.0",
		"@types/node": "12.7.9",
		"@types/react": "16.9.4",
		"@types/react-dom": "16.9.1",
		"@types/react-hot-loader": "4.1.0",
		"@types/react-inlinesvg": "0.8.1",
		"@types/react-loadable": "5.5.1",
		"@types/react-redux": "7.1.4",
		"@types/react-router": "5.1.1",
		"@types/react-router-dom": "5.1.0",
		"@types/react-test-renderer": "16.9.0",
		"@types/styled-components": "^4.1.19",
		"@types/terser-webpack-plugin": "1.2.1",
		"@types/webpack": "4.39.2",
		"@types/webpack-dev-server": "3.1.7",
		"autoprefixer": "9.6.1",
		"copy-webpack-plugin": "5.0.4",
		"cross-env": "^6.0.3",
		"css-hot-loader": "1.4.4",
		"css-loader": "3.2.0",
		"cssnano": "4.1.10",
		"dotenv-webpack": "^1.7.0",
		"enzyme": "3.10.0",
		"enzyme-adapter-react-16": "1.14.0",
		"enzyme-to-json": "3.4.0",
		"file-loader": "4.2.0",
		"html-loader": "0.5.5",
		"html-webpack-plugin": "3.2.0",
		"husky": "^3.0.8",
		"image-webpack-loader": "6.0.0",
		"jest": "24.9.0",
		"lint-staged": "^9.4.2",
		"mini-css-extract-plugin": "0.8.0",
		"node-sass": "4.12.0",
		"offline-plugin": "5.0.7",
		"postcss": "7.0.18",
		"postcss-easy-import": "3.0.0",
		"postcss-flexbugs-fixes": "4.1.0",
		"postcss-loader": "3.0.0",
		"postcss-url": "8.0.0",
		"postcss-utilities": "0.8.4",
		"prop-types": "15.7.2",
		"react-hot-loader": "4.12.14",
		"react-test-renderer": "16.10.1",
		"rimraf": "^3.0.0",
		"sass-loader": "8.0.0",
		"style-loader": "1.0.0",
		"stylelint": "11.0.0",
		"stylelint-config-recommended": "3.0.0",
		"svg-inline-loader": "0.8.0",
		"terser-webpack-plugin": "2.1.2",
		"ts-jest": "24.1.0",
		"ts-loader": "6.2.0",
		"ts-node": "8.4.1",
		"tslint": "5.20.0",
		"tslint-config-airbnb": "^5.11.2",
		"tslint-config-prettier": "1.18.0",
		"tslint-react": "4.1.0",
		"tslint-react-hooks": "^2.2.1",
		"typescript": "3.6.3",
		"url-loader": "2.1.0",
		"webpack": "4.41.0",
		"webpack-cli": "3.3.9",
		"webpack-dev-server": "3.8.1",
		"webpack-hot-middleware": "2.25.0",
		"webpack-pwa-manifest": "4.0.0"
	},
	"husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
		"*.{js,jsx}": [
			"npm run lint:ts",
			"npm run test:update",
			"git add"
		]
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"jest": {
		"moduleDirectories": [
			"node_modules",
			"test",
			"src"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1",
			"^@src/(.*)$": "<rootDir>/src/$1",
			"^@root/(.*)$": "<rootDir>/$1",
			"^@assets/(.*)$": "<rootDir>/src/assets/$1",
			"^@utilities/(.*)$": "<rootDir>/src/utilities/$1",
			"^@components/(.*)$": "<rootDir>/src/components/$1",
			"^@containers/(.*)$": "<rootDir>/src/containers/$1",
			"\\.(css|less|sass|scss)$": "<rootDir>/test/test-config/StyleMock.js",
			"\\.(jpg|jpeg|png|gif|ico|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/test-config/FileMock.js"
		},
		"setupFiles": [
			"<rootDir>/test/test-config/index.js",
			"<rootDir>/test/test-config/LocalStorageMock.js"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest",
			"\\.(css|less|sass|scss)$": "<rootDir>/test/test-config/StyleMock.js",
			"\\.(jpg|jpeg|png|gif|ico|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/test-config/FileMock.js"
		},
		"verbose": false,
		"testRegex": ".*\\.(test|spec)\\.(j|tsx?)$",
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/",
			"<rootDir>/dist/",
			"<rootDir>/test/test-config/"
		]
	},
	"private": true
}
